<p id="notice"><%= notice %></p>

<h1>Video2tags</h1>

<table>
  <thead>
    <tr>
      <th>Video</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video2tags.each do |video2tag| %>
      <tr>
        <td><%= video2tag.video_id %></td>
        <td><%= video2tag.tag_id %></td>
        <td><%= link_to 'Show', video2tag %></td>
        <td><%= link_to 'Edit', edit_video2tag_path(video2tag) %></td>
        <td><%= link_to 'Destroy', video2tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Video2tag', new_video2tag_path %>
